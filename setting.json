  "editor.tokenColorCustomizations": {
    // コメントの文字色
    "comments": {
      "foreground": "#ac3838"
    },
    // 関数名の色
    "functions": "#005cc5",
    // if, for などのキーワード
    "keywords": "#f07582",
    // 数値リテラルの色
    "numbers": "#0f70b6",
    // 文字列リテラルの色
    "strings": "#22863a",
    // 型（TypeScriptの型など）の色
    "types": "#d58603",
    // 変数名の色
    "variables": "#e36209",

    // より細かなスコープごとの色設定
    "textMateRules": [
      {
        // JSONのキー部分の色
        "scope": "support.type.property-name.json",
        "settings": {
          "foreground": "#907a23"
        }
      },
      {
        "scope": "keyword.operator", // ピリオドやコロンなど
        "settings": {
          "foreground": "#4c3d4c", // 例: マゼンタに変更
          "fontStyle": "bold"
        }
      },
      {
        "scope": "meta.embedded.block", // ピリオドやコロンなど
        "settings": {
          "foreground": "#7e437e", // 例: マゼンタに変更
          "fontStyle": "bold"
        }
      },
      {
        // PowerShellの制御構文（if, goto など）
        "scope": "keyword.control.ps1",
        "settings": {
          "foreground": "#ff557f",
          "fontStyle": "bold"
        }
      }
    ]
  },

